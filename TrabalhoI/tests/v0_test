Teste 1

int: avestruz, burro, cachorro;
real: dragao, elefante;
bool: foca, golfinho;
avestruz := 1 + 3 / 5;
burro := avestruz - 100;
cachorro := - ( burro );
dragao := 10.0 - ( 2. + .7 );
elefante := - avestruz;
foca := TRUE AND avestruz <= burro;
golfinho := ~ ( 5 > elefante );


Teste 2

bool: var;
var := TRUE;
var := FALSE;
var := ~var;
var := var AND var;
var := var OR var;
var := 1 = 7;
var := 2.0 ~= 8.0;
var := 3 < 9;
var := 4 > 10;
var := 5 <= 11;
var := 6 >= 12;

Teste 3

int: var;
real: other;
var := 1;
var := -var;
var := 2 + 3 * 4 / 5 -6;
var := ( 2 + 3 ) * 4 / 5 - 6;
other := 0.;
other := .000000;
other := 0.0;
other := 6.0 * ( 7.0 + 8.0) / 9.0 - 10.0;
other := 6 * ( 7 + 8 ) / 9 - 10;
